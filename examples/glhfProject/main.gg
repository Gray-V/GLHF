Gameloop intro()
    load()
        File.parent.start_menu
    end load
    ~on_click
        start_button -> main()
        settings_button ->
            settings()
end on_quit


Gameloop main()
    load()
        File.Parent.settings.default() --comes from settings.gg
    end load
    Wait(5) --milliseconds
    spawn(Parent.player.player_controller.player1)

end on_quit